Create Table chicken (
	Year INT NOT NULL,
	Period VARCHAR (10) NOT NULL,
	PeriodName VARCHAR (20) NOT NULL,
	Value FLOAT NOT NULL,
	Footnotes VARCHAR,
	SeriesID VARCHAR(20)
);

select * from chicken

Create Table eggs (
	Year INT NOT NULL,
	Period VARCHAR (10) NOT NULL,
	PeriodName VARCHAR (20) NOT NULL,
	Value FLOAT NOT NULL,
	Footnotes VARCHAR,
	SeriesID VARCHAR(20)
);

Create Table milk (
	Year INT NOT NULL,
	Period VARCHAR (10) NOT NULL,
	PeriodName VARCHAR (20) NOT NULL,
	Value FLOAT NOT NULL,
	Footnotes VARCHAR,
	SeriesID VARCHAR(20)
);

drop table cheese

Create Table cheese (
	Year INT NOT NULL,
	Period VARCHAR (10) NOT NULL,
	PeriodName VARCHAR (20) NOT NULL,
	Value FLOAT NOT NULL,
	Footnotes VARCHAR,
	SeriesID VARCHAR(20)
);

SELECT * FROM cheese